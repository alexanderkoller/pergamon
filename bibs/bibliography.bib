@inproceedings{lai-etal-2020-continuation,
    title = "A Continuation Semantics for {A}bstract {M}eaning {R}epresentation",
    author = "Lai, Kenneth  and
      Donatelli, Lucia  and
      Pustejovsky, James",
    editor = "Xue, Nianwen  and
      Bos, Johan  and
      Croft, William  and
      Haji{\v{c}}, Jan  and
      Huang, Chu-Ren  and
      Oepen, Stephan  and
      Palmer, Martha  and
      Pustejovsky, James",
    booktitle = "Proceedings of the Second International Workshop on Designing Meaning Representations",
    month = dec,
    year = "2020",
    keywords = {workshop},
    address = "Barcelona Spain (online)",
    publisher = "Association for Computational Linguistics",
    url = "https://aclanthology.org/2020.dmr-1.1",
    pages = "1--12",
    abstract = "Abstract Meaning Representation (AMR) is a simple, expressive semantic framework whose emphasis on predicate-argument structure is effective for many tasks. Nevertheless, AMR lacks a systematic treatment of projection phenomena, making its translation into logical form problematic. We present a translation function from AMR to first order logic using continuation semantics, which allows us to capture the semantic context of an expression in the form of an argument. This is a natural extension of AMR{'}s original design principles, allowing us to easily model basic projection phenomena such as quantification and negation as well as complex phenomena such as bound variables and donkey anaphora.",
}


@inproceedings{anikina-etal-2020-predicting,
    title = "Predicting Coreference in {A}bstract {M}eaning {R}epresentations",
    author = "Anikina, Tatiana  and
      Koller, Alexander  and
      Roth, Michael",
    booktitle = "Proceedings of the Third Workshop on Computational Models of Reference, Anaphora and Coreference",
    year = "2020",
    keywords = {workshop},
    url = "https://www.aclweb.org/anthology/2020.crac-1.4",
    pages = "33--38",
    abstract = "This work addresses coreference resolution in Abstract Meaning Representation (AMR) graphs, a popular formalism for semantic parsing. We evaluate several current coreference resolution techniques on a recently published AMR coreference corpus, establishing baselines for future work. We also demonstrate that coreference resolution can improve the accuracy of a state-of-the-art semantic parser on this corpus.",
}





